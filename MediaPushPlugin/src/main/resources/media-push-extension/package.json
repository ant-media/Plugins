{
  "name": "media-push-extension",
  "version": "0.1.0",
  "description": "Ant Media Media Push Extension",
  "private": true,
  "scripts": {
    "watch": "webpack --mode=development --watch --config config/webpack.config.js",
    "build": "webpack --mode=production --config config/webpack.config.js",
    "pack": "node pack.js",
    "repack": "npm run build && npm run pack",
    "format": "prettier --write --ignore-unknown \"{config,public,src}/**/*.{html,css,js,ts,json}\""
  },
  "dependencies": {
    "@antmedia/webrtc_adaptor": "^2.8.0-SNAPSHOT-2023-Dec-20-10-46"
  },
  "devDependencies": {
    "adm-zip": "^0.5.10",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.8.8",
    "webpack": "^5.89.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.10.0"
  }
}
